{
  "name": "lkt/influx-connector",
  "description": "LKT InfluxDB connector",
  "type": "library",
  "version": "1.0.5",
  "require": {
    "php": ">=8.1.0",
    "ext-pdo": "*",
    "lkt/database-connector": "^2.1",
    "lkt/locale": "^2.0",
    "lkt/query-builder": "^2.0",
    "influxdata/influxdb-client-php": "^3.2",
    "php-http/guzzle7-adapter": "^1.0",
    "nyholm/psr7": "^1.5",
    "php-http/curl-client": "^2.2"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5",
    "symfony/var-dumper": "^6.2"
  },
  "license": "MIT",
  "autoload": {
    "psr-4": {
      "Lkt\\Connectors\\": "src",
      "Lkt\\Connectors\\Tests\\": "tests"
    }
  },
  "config": {
    "allow-plugins": {
      "php-http/discovery": true
    }
  }
}
